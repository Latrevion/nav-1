00%;\r\n  height: 100%;\r\n  opacity: 1;\r\n  object-fit: cover;\r\n  /* filter: blur(10px); */\r\n}\r\n\r\n.cover {\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  z-index: -1;\r\n  width: 100%;\r\n  height: 100%;\r\n  opacity: 1;\r\n}\r\n\r\n.globalHeader {\r\n  /* border: 1px solid red; */\r\n  margin: 20px;\r\n}\r\n\r\n@media (min-width: 500px) {\r\n  .globalHeader {\r\n    margin: 60px 0 80px;\r\n  }\r\n}\r\n\r\n.searchForm {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n@media (min-width: 500px) {\r\n  .searchForm {\r\n    max-width: 400px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n}\r\n\r\n.searchForm > input {\r\n  width: 100%;\r\n  margin-right: 10px;\r\n  height: 40px;\r\n  padding: 0 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  caret-color: rgb(6, 156, 156);\r\n  border-color: rgb(6, 156, 156);\r\n  outline-color: rgb(6, 156, 156);\r\n}\r\n.searchForm > button {\r\n  white-space: nowrap;\r\n  padding: 0 28px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  background: #069ca6;\r\n  color: white;\r\n  font-size: 16px;\r\n}\r\n\r\n.globalMain {\r\n  max-width: 900px;\r\n  margin-right: auto;\r\n  margin-left: auto;\r\n}\r\n\r\n.siteList {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  /* border: 1px solid black; */\r\n  margin: 20px;\r\n  justify-content: space-between;\r\n  /* border: 1px solid red; */\r\n}\r\n\r\n@media (min-width: 500px) {\r\n  .siteList {\r\n    margin-right: -25px;\r\n    margin-left: 0;\r\n    justify-content: flex-start;\r\n  }\r\n}\r\n\r\n.siteList > li {\r\n  /* border: 1px solid red; */\r\n  margin-bottom: 10px;\r\n}\r\n@media (min-width: 500px) {\r\n  .siteList > li {\r\n    margin-right: 25px;\r\n  }\r\n}\r\n\r\n.siteList .site {\r\n  width: 160px;\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  background: white;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  padding: 20px 0;\r\n  position: relative;\r\n  cursor: pointer;\r\n  opacity: 0.8;\r\n}\r\n.siteList .site > .logo {\r\n  /* border: 1px solid red; */\r\n  width: 64px;\r\n  height: 64px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 64px;\r\n  text-transform: uppercase;\r\n}\r\n.siteList .site > .link {\r\n  font-size: 14px;\r\n  margin-top: 4px;\r\n}\r\n.siteList .site > .close {\r\n  position: absolute;\r\n  right: 2px;\r\n  top: 2px;\r\n  font-size: 30px;\r\n  display: none;\r\n  cursor: default;\r\n}\r\n.siteList .site:hover > .close {\r\n  display: block;\r\n}\r\n\r\n.siteList .addButton {\r\n  border: 1px solid #ddd;\r\n  background: white;\r\n  width: 160px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  padding: 20px 0;\r\n  opacity: 0.8;\r\n  border-radius: 4px;\r\n}\r\n.siteList .addButton .icon {\r\n  width: 64px;\r\n  height: 64px;\r\n}\r\n\r\n.siteList .addButton .text {\r\n  font-size: 14px;\r\n  margin-top: 4px;\r\n}\r\n\r\n.siteList .addButton .icon-wrapper {\r\n  /* border: 1px solid red; */\r\n  width: 64px;\r\n  height: 64px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\nfooter {\r\n  color: rgba(255, 255, 255, 0.6);\r\n  position: fixed;\r\n  bottom: 10px;\r\n  left: 50%;\r\n  margin-left: -68px;\r\n  /* border: 1px solid red; */\r\n}\r\n"],"names":[],"version":3,"file":"index.HASH_REF_f0918a547ace1733ebc251b0de60a1d7.css.map"}